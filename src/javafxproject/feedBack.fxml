<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="509.0" prefWidth="629.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxproject.FeedBackController">
   <children>
      <TextField fx:id="nameTxt" focusTraversable="false" layoutX="51.0" layoutY="75.0" prefHeight="26.0" prefWidth="225.0" promptText="Your Name" />
      <TextField fx:id="BranchTxt" focusTraversable="false" layoutX="51.0" layoutY="125.0" prefHeight="26.0" prefWidth="226.0" promptText="Branch" />
      <TextField fx:id="idTxt" focusTraversable="false" layoutX="330.0" layoutY="75.0" prefHeight="26.0" prefWidth="226.0" promptText="AccountID(if any)" />
      <TextField fx:id="PhoneNoTxt" focusTraversable="false" layoutX="330.0" layoutY="125.0" prefHeight="26.0" prefWidth="226.0" promptText="Phone No" />
      <TextField fx:id="emailTxt" focusTraversable="false" layoutX="51.0" layoutY="168.0" prefHeight="26.0" prefWidth="226.0" promptText="Email Address" />
      <TextField fx:id="addressTxt" focusTraversable="false" layoutX="52.0" layoutY="220.0" prefHeight="34.0" prefWidth="542.0" promptText="Your Address" />
      <TextArea fx:id="msgTxt" focusTraversable="false" layoutX="52.0" layoutY="278.0" prefHeight="184.0" prefWidth="542.0" promptText="Write Message" />
      <Button fx:id="SubmitBtn" focusTraversable="false" layoutX="539.0" layoutY="469.0" mnemonicParsing="false" onAction="#SubmitBtnOnClick" text="Submit" />
      <Label focusTraversable="false" layoutX="266.0" layoutY="24.0" text="Feedback">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Button fx:id="back" focusTraversable="false" layoutX="539.0" layoutY="469.0" mnemonicParsing="false" onAction="#backBtnOnclick" text="Back" visible="false" />
   </children>
</AnchorPane>
