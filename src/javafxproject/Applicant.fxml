<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="615.0" prefWidth="905.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxproject.ApplicantController">
   <children>
      <Button fx:id="openLc" layoutX="62.0" layoutY="58.0" mnemonicParsing="false" onAction="#createLc" text="Open LC" />
      <Pane fx:id="paneform" layoutX="251.0" layoutY="58.0" prefHeight="505.0" prefWidth="610.0" visible="false">
         <children>
            <TextField fx:id="sellername" layoutX="36.0" layoutY="39.0" promptText="Enter Seller Name" />
            <TextField fx:id="contactno" layoutX="36.0" layoutY="81.0" promptText="Enter Seller Contact No" />
            <TextField fx:id="bussinessname" layoutX="36.0" layoutY="123.0" promptText="Registerd Bussiness Name" />
            <TextField fx:id="email" layoutX="36.0" layoutY="167.0" promptText="Mailing Address" />
            <Button fx:id="LcSubmit" layoutX="545.0" layoutY="467.0" mnemonicParsing="false" onAction="#submitLC" text="Submit" />
         </children>
      </Pane>
      <Button fx:id="submission" layoutX="62.0" layoutY="107.0" mnemonicParsing="false" onAction="#showallLc" prefHeight="26.0" prefWidth="83.0" text="Submissions" />
      <TextArea fx:id="ScrollLabel" editable="false" layoutX="350.0" layoutY="32.0" prefHeight="557.0" prefWidth="527.0" visible="false" />
      <Button fx:id="edit" layoutX="744.0" layoutY="550.0" mnemonicParsing="false" onAction="#editLc" prefHeight="26.0" prefWidth="62.0" text="Edit" visible="false" />
      <Button fx:id="ok" layoutX="814.0" layoutY="550.0" mnemonicParsing="false" onAction="#editok" prefHeight="26.0" prefWidth="62.0" text="Ok" visible="false" />
      <Button fx:id="Doc" layoutX="62.0" layoutY="156.0" mnemonicParsing="false" onAction="#DocBtnOnCLick" prefHeight="26.0" prefWidth="122.0" text="Documentation" />
      <Button fx:id="logout" layoutX="62.0" layoutY="538.0" mnemonicParsing="false" onAction="#loggedout" text="Log Out" />
      <Button fx:id="download" layoutX="664.0" layoutY="550.0" mnemonicParsing="false" onAction="#saveEtittedTextButtonOnClick" text="Download" visible="false" />
      <Button fx:id="profile" layoutX="62.0" layoutY="208.0" mnemonicParsing="false" onAction="#viewProfile" text="Profile" />
   </children>
</AnchorPane>
